{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "-webkit-tap-highlight-color",
          "-ms-touch-action",
          "backdrop-filter",
          "-moz-appearance",
          "width: fit-content"
        ]
      }
    ],
    "disown-opener": "off",
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[fetchpriority]",
          "input[autocapitalize]"
        ]
      }
    ],
    "meta-viewport": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ]
  }
}